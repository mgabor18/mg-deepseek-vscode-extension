{
  "name": "mg-deepseek-vscode-extension",
  "displayName": "mg-deepseek-vscode-extension",
  "description": "This is an extension to be able to chat with Deepseek inside VSCode.",
  "url": "https://github.com/mgabor18/mg-deepseek-vscode-extension.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgabor18/mg-deepseek-vscode-extension.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mg-deepseek-vscode-extension.start",
        "title": "Run: DeepSeek Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ollama": "^0.5.12"
  }
}
